{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "icons" : {
    "64"  : "spy64.png",
    "128" : "spy128.png"
  },

  "browser_action": {
    "default_icon": "spy64.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
